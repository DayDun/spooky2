================
Mandelbrot set
================
extern print(c: Char)
extern printInt(n: Int)

func mandel(Cx: Int, Cy: Int, max: Int) -> Int {
	Qx := 0;
	Qy := 0;
	x := Cx;
	y := Cy;
	
	i: Int;
	for (i = 0; i < max; i++) {
		Qx = (x * x) / 4096;
		Qy = (y * y) / 4096;
		
		if (Qx + Qy > 4 * 4096) break;
		
		y = ((x * y) / 2048) + Cy;
		x = Qx - Qy + Cx;
	}
	
	return i;
}

func main() {
	palette: [10]Int;
	
	max := 9;
	for (y := -8192; y < 8192; y += 512) {
		for (x := -8192; x < 8192; x += 256) {
			m := mandel(x, y, max);
			printInt(m);
		}
		print('\n');
	}
}

